#! /bin/sh

#set -e

case "$1" in
	   remove|deconfigure)
		if [ -x `which gconf-schemas` ];then
			gconf-schemas --unregister /usr/share/gconf/schemas/klik-gnome-thumbnail.schemas >/dev/null
		fi
		if [ -x `which update-binfmts` ];then
			update-binfmts --remove cmg /usr/bin/zAppRun >/dev/null
		fi
		;;
       upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
